{
  "name": "solbet_backend",
  "version": "1.0.0",
  "description": "SolBet backend",
  "main": "app.ts",
  "author": "superteam delta",
  "license": "MIT",
  "scripts": {
    "build": "tsc && yarn sentry:sourcemaps",
    "start": "node --env-file=.env build/index.js | pino-pretty --colorize",
    "start:dev": "ts-node-dev --env-file=.env src/index.ts | pino-pretty --colorize",
    "lint": "eslint . --ext .ts --ext .mts --fix",
    "prettier": "prettier --config .prettierrc --write src",
    "db:gen": "prisma generate",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:format": "prisma format",
    "test": "echo \\\"Error: no src/test specified\\\" && exit 1"
  },
  "dependencies": {
    "@prisma/client": "5.9.1",
    "axios": "^1.7.7",
    "bunyan": "1.8.15",
    "express": "^4.19.2",
    "helmet": "7.1.0",
    "jsonwebtoken": "9.0.2",
    "micromatch": "^4.0.8",
    "morgan": "1.10.0",
    "node-cron": "^3.0.3",
    "pino-pretty": "10.3.1",
    "prisma": "5.9.1",
    "winston": "3.11.0",
    "yup": "1.3.3"
  },
  "devDependencies": {
    "@types/bunyan": "1.8.11",
    "@types/express": "4.17.21",
    "@types/is-ci": "3.0.4",
    "@types/jsonwebtoken": "^9",
    "@types/micromatch": "^4",
    "@types/morgan": "1.9.9",
    "@types/node": "22.2.0",
    "@types/node-cron": "^3",
    "@types/sinon": "17.0.3",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "docker-compose": "0.24.3",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.0.10",
    "is-ci": "3.0.1",
    "is-port-reachable": "4.0.0",
    "nock": "13.5.1",
    "nodemon": "3.0.3",
    "prettier": "3.2.4",
    "sinon": "17.0.1",
    "supertest": "6.3.4",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.3.3"
  }
}
